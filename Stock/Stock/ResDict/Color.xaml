<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <LinearGradientBrush x:Key="MainColorBrush" StartPoint="0,0" EndPoint="1,1" x:Shared="false">
        <GradientStop Color="LightSkyBlue" Offset="0"/>
        <GradientStop Color="PowderBlue" Offset="0.25"/>
        <GradientStop Color="LightSkyBlue" Offset="0.5"/>
        <GradientStop Color="PowderBlue" Offset="0.75"/>
        <GradientStop Color="LightSkyBlue" Offset="1"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="BoxColor" Color="AliceBlue" x:Shared="false"/>
    <SolidColorBrush x:Key="CanvasColor" Color="White" x:Shared="false"/>
    
    
</ResourceDictionary>